RewriteEngine On

RewriteCond %{REQUEST_URI} !^/$
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]

RewriteCond %{REQUEST_URI} ^/impressum$
RewriteRule ^(.*)$ impressum.php [NC,L]

RewriteCond %{REQUEST_URI} ^/presse$
RewriteRule ^(.*)$ presse.php [NC,L]

RewriteCond %{REQUEST_URI} ^/datenschutz$
RewriteRule ^(.*)$ datenschutz.php [NC,L]

RewriteCond %{REQUEST_URI} ^/projekt$
RewriteRule ^(.*)$ projekt.php [NC,L]

RewriteCond %{REQUEST_URI} ^/$
RewriteRule ^.*$ /index.php [L]

ErrorDocument 404 /404.php

